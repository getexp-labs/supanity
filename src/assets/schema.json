  "definitions": {
    "discounts": {
      "required": ["id", "type"],
      "properties": {
        "id": {
          "format": "uuid",
          "type": "string",
          "description": "Note:\nThis is a Primary Key.<pk/>"
        },
        "amount": {
          "format": "integer",
          "type": "integer",
          "description": "Amount of the discount, meaning depends on type"
        },
        "type": {
          "format": "character varying",
          "type": "string",
          "description": "Type of the discount (e.g. 2_FOR_1, PERCENT, 3_FOR_1)"
        },
        "valid_till": {
          "format": "timestamp without time zone",
          "type": "string",
          "description": "End of discount for limited ones"
        }
      },
      "type": "object"
    },
    "games": {
      "required": ["id", "name", "platforms"],
      "properties": {
        "id": {
          "format": "uuid",
          "type": "string",
          "description": "Note:\nThis is a Primary Key.<pk/>"
        },
        "name": {
          "format": "character varying",
          "type": "string",
          "description": "Name of the game"
        },
        "platforms": {
          "format": "ARRAY",
          "type": "string",
          "description": "Platforms where game is available (e.g. PS4/PS5/PC/XBOX)"
        }
      },
      "type": "object"
    },
    "likes": {
      "required": ["user_id", "offer_id"],
      "properties": {
        "user_id": {
          "format": "uuid",
          "type": "string",
          "description": "Note:\nThis is a Foreign Key to `users.id`.<fk table='users' column='id'/>"
        },
        "offer_id": {
          "format": "uuid",
          "type": "string",
          "description": "Note:\nThis is a Foreign Key to `offers.id`.<fk table='offers' column='id'/>"
        }
      },
      "type": "object"
    },
    "offer_groups": {
      "required": ["id", "name"],
      "properties": {
        "id": {
          "format": "uuid",
          "type": "string",
          "description": "Note:\nThis is a Primary Key.<pk/>"
        },
        "name": {
          "format": "character varying",
